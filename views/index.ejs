    .btn:hover{
        background: var(--main-color);

    }
    .heading-title{
        text-align: center;
        margin-bottom: 3rem;
        font-size: 6rem;
        text-transform: uppercase;
        color: var(--black);
    }
    .header{
        position: sticky;
        top:0; left:0; right:0;
        z-index: 1000;
        background-color: var(--white);
        display:flex;
        padding-top: 2rem;
        padding-bottom: 2rem;
        box-shadow: var(--box-shadow);
        align-items:center;
