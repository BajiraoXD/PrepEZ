}

#menu-btn{
    font-size: 2.5rem; 
    cursor: pointer;
    color: var(--black);
    display: none;
}
   /*nav bar modify*/
    /* Base styles for navbar links */
.navbar a {
    display: inline-block;
    padding: 3px 7px;
    font-size: 1rem;
    color: #333;
    text-decoration: none;
    transition: transform 0.3s ease, color 0.3s ease;
    position: relative;
}

/* Zoom-in effect on hover */
.navbar a:hover {
    transform: scale(1.1); /* Slight zoom-in effect */
    color: #007bff; /* Change color on hover */
}

/* Optional: Add a subtle underline on hover */
.navbar a::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: -5px;
    width: 100%;
    height: 2px;
    background: #007bff;
    transform: scaleX(0);
    transition: transform 0.3s ease;
}

.navbar a:hover::after {
    transform: scaleX(1);
}

