:root{
    --main-color: #8e44ad;
    --black:#222;
    --white:#fff;
    --light-black: #777;
    --light-white: #fff9;
    --dark-bg: rgba(0,0,0,.7);
    --light-bg: #eee;
    --border:  .1rem solid var(--black);
    --box-shadow:0 .5rem 1rem rgba(0,0,0,.1);
