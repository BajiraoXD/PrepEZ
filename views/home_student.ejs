}
.logo img{
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.logo h1 {
  margin-left: 1rem;
  transition: color 0.3s ease;
  display: inline-block;
}

/* Define zoom keyframes */
@keyframes zoom {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.2);
  }
  100% {
    transform: scale(1);
  }
}

.logo h1:hover {
  color: #a68efd;
  animation: zoom 0.6s ease-in-out; /* Apply zoom animation */
}


ul{
  margin: 0 auto;
}
li{
  padding-bottom: 2rem;
}
li a{
